<!DOCTYPE Dataset [
    <!-- Dataset is the parent value -->
    <!ELEMENT Dataset (id, clump_thickness, uniformity_cell_size, uniformity_cell_shape, marginal_adhesion, 
        single_epithelial_cell_size, bare_nuclei, bland_chromatin, normal_nucleoli, mitoses, class) >
    <!-- Element Format: Name of variable (Type of data) -->
    <!ELEMENT id (#PCDATA) >
    <!ELEMENT clump_thickness (#PCDATA) >
    <!ELEMENT uniformity_cell_size (#PCDATA) >
    <!ELEMENT uniformity_cell_shape (#PCDATA) >
    <!ELEMENT marginal_adhesion (#PCDATA) >
    <!ELEMENT single_epithelial_cell_size (#PCDATA) >
    <!ELEMENT bare_nuclei (#PCDATA) >
    <!ELEMENT bland_chromatin (#PCDATA) >
    <!ELEMENT normal_nucleoli (#PCDATA) >
    <!ELEMENT mitoses (#PCDATA) >
    <!ELEMENT class (#PCDATA) > <!-- 2 for benign, 4 for malignant -->
]>